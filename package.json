{
  "name": "aft-examples",
  "version": "4.0.0",
  "description": "Automated Functional Testing (AFT) package containing examples of how it can be used",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc --declaration",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/jasmine JASMINE_CONFIG_PATH=./jasmine.json",
    "coverage": "nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bicarbon8/aft-examples.git"
  },
  "keywords": [
    "aft",
    "automation",
    "e2e",
    "integration",
    "functional",
    "ui",
    "logging",
    "test",
    "examples"
  ],
  "author": "Jason Holt Smith <bicarbon8@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bicarbon8/aft-examples/issues"
  },
  "homepage": "https://github.com/bicarbon8/aft-examples#readme",
  "devDependencies": {
    "@types/jasmine": "3.4.6",
    "@types/node": "12.12.7",
    "@types/selenium-webdriver": "4.0.5",
    "jasmine": "3.5.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "typescript": "3.7.2"
  },
  "dependencies": {
    "aft-logging-awskinesis": "^4.0.0",
    "aft-testrail": "^4.0.0",
    "aft-ui-selenium": "^4.0.0",
    "es6-promise": "4.2.8"
  }
}
